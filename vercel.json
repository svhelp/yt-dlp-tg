{
  "functions": {
    "api/webhook.py": { "runtime": "python3.11" },
    "api/index.py": { "runtime": "python3.11" }
  },
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.py",
      "methods": ["GET", "POST"]
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ]
}
